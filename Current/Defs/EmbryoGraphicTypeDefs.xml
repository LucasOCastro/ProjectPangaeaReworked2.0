<?xml version="1.0" encoding="utf-8" ?>

<Defs>
	
	<ProjectPangaea.ResourceGraphicDef Abstract="True" Name="EmbryoGraphicBase">
		<defName>EmbryoGraphicBase</defName>
		<resourceType>Pangaea_Embryo</resourceType>
		<graphicData>
			<graphicClass>Graphic_Single</graphicClass>
			<shaderType>PangaeaEmbryoShader</shaderType>
			<shaderParameters>
				<_EmbryoOffset>(0,0)</_EmbryoOffset>
				<_FlaskTop>/Things/Item/Embryos/Flask/EmbryoFlaskTop</_FlaskTop>
				<_FlaskBase>/Things/Item/Embryos/Flask/EmbryoFlaskBot</_FlaskBase>
			</shaderParameters>
		</graphicData>
	</ProjectPangaea.ResourceGraphicDef>
	
